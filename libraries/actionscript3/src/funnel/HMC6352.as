package funnel {	import funnel.I2CDevice;	/**	 * This is the class to express HMC6352 devices	 *	 */	public class HMC6352 extends I2CDevice {		public var heading:Number;		public function HMC6352(ioModule:IOModule, address:uint = 0x21) {			super(ioModule, address);			heading = 0;			_io.sendSysex(0x76, [WRITE, address, 'G', 0x74, 0x51]);			_io.sendSysex(0x76, [WRITE, address, 'A']);			_io.sendSysex(0x76, [READ_CONTINUOUS, address, 0x7F, 0x02]);		}		public override function handleSysex(command:uint, data:Array) {			if (command != 0x76) {				return;			}			heading = (int(data[2]) * 256 + int(data[3])) / 10.0;		}	}}