package {	import flash.display.Shape;	import flash.display.Sprite;	import flash.events.Event;	import flash.events.MouseEvent;	import funnel.*;	public class AnalogOutTest extends Sprite {		private var gio:Gainer;		private var osc:Osc;		public function AnalogOutTest() {			gio = new Gainer();			var sprite:Sprite = new Sprite();			addChild(sprite);			sprite.graphics.beginFill(0x000000);			sprite.graphics.drawCircle(275, 200, 100);			sprite.graphics.endFill();			sprite.addEventListener(MouseEvent.MOUSE_DOWN, mousePressed);			sprite.addEventListener(MouseEvent.MOUSE_UP, mouseReleased);			sprite.buttonMode = true;			osc = new Osc(Osc.SQUARE, 1.0);			gio.analogOutput(0).filters = [osc];		}		private function mousePressed(e:MouseEvent):void {			osc.reset();			osc.start();		}		private function mouseReleased(e:MouseEvent):void {			osc.stop();			gio.analogOutput(0).value = 0;		}	}}