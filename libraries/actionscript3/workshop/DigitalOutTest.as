package {	import flash.display.Shape;	import flash.display.Sprite;	import flash.events.Event;	import flash.events.MouseEvent;	import funnel.*;		public class DigitalOutTest extends Sprite {		private var gio:Gainer;		private var osc:Osc;				public function DigitalOutTest() {			gio = new Gainer();			var sprite:Sprite = new Sprite();			addChild(sprite);			sprite.graphics.beginFill(0x000000);			sprite.graphics.drawCircle(275, 200, 100);			sprite.addEventListener(MouseEvent.MOUSE_DOWN, mousePressed);			sprite.addEventListener(MouseEvent.MOUSE_UP, mouseReleased);						// Uncomment the following lines to activate the oscillator			// オシレータを有効にするには次の行をアンコメントする			osc = new Osc(Osc.SQUARE, 2.0);			gio.digitalOutput(0).filters = [osc];		}				private function mousePressed(e:Event):void {			gio.led.value = 1;						// Uncomment the following line to control the digital output 0			// デジタル出力0をコントロールするには次の行をアンコメントする			gio.digitalOutput(0).value = 1;			// Uncomment the following lines to activate the oscillator			// オシレータを有効にするには次の行をアンコメントする			osc.reset();			osc.start();		}		private function mouseReleased(e:Event):void {			gio.led.value = 0;			// Uncomment the following line to control the digital output 0			// デジタル出力0をコントロールするには次の行をアンコメントする			gio.digitalOutput(0).value = 0;			// Uncomment the following lines to activate the oscillator			// オシレータを有効にするには次の行をアンコメントする			osc.stop();			gio.led.value = 0;		}	}}