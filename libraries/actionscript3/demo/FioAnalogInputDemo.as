package {	import flash.display.*;	import flash.events.Event;	import funnel.*;	public class FioAnalogInputDemo extends Sprite{		private var fio:Fio;		public function FioAnalogInputDemo() {			var config:Configuration = Fio.FIRMATA;			fio = new Fio([1], config, "127.0.0.1");			var sensorPin:Port = fio.ioModule(1).port(14);			var circle:Shape = new Shape();			circle.graphics.beginFill(0x000000);			circle.graphics.drawEllipse(225, 150, 100, 100);			circle.graphics.endFill();			circle.visible = false;			this.addChild(circle);			sensorPin.filters = [new SetPoint([[0.5, 0.1]])];			sensorPin..addEventListener(PortEvent.RISING_EDGE, function(e:Event):void {				circle.visible = false;			});			sensorPin..addEventListener(PortEvent.FALLING_EDGE, function(e:Event):void {				circle.visible = true;			});		}	}}