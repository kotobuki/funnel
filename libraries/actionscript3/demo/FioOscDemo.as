package {	import flash.display.*;	import funnel.*;		public class FioOscDemo extends Sprite{		private var fio:Fio;    // NOTE: current version treats D0 as port 8		private var D3:int = 11;  // R		private var D10:int = 18; // G		private var D11:int = 19; // B				public function FioOscDemo() {		  // supply module ids to control individually			fio = new Fio([1], "127.0.0.1");						// wave, freq, amp = 1, offset = 0, phase = 0, times = 0 (0 means infinite)      var osc1:Osc = new Osc(Osc.SIN, 0.5, 1, 0, 0);      var osc2:Osc = new Osc(Osc.SIN, 0.5, 1, 0, 0.5);			fio.ioModule(1).port(D10).filters = [osc1];			fio.ioModule(1).port(D11).filters = [osc2];			osc1.start();			osc2.start();		}	}}